{
	"presets": ["env", "react", "stage-2"],
	// "env": {
	// 	"development": {
	// 		"presets": [
	// 			"react-hmre"
	// 		]
	// 	}
	// },
	"env": {
		"development": {
			"plugins": ["react-hot-loader/babel"]
		}
	},
	"plugins": [
		[
			"transform-runtime",
			{
				"helpers": false,
				"polyfill": false,
				"regenerator": true
			}
		],
		"transform-proto-to-assign",
		"transform-object-assign",
		"transform-class-bound-properties",
		"transform-class-properties"
	] //, "react-hot-loader/babel"]
}
